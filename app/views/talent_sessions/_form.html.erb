
<%= form_for(@talent, url: talent_sessions_new_path, local: true) do |f| %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email %>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </div>

  <div class="form-group">
    <%= f.submit("Login",class: "btn btn-primary") %>
  </div>

<% end %>