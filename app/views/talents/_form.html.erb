
<%= form_for(@talent, local: true) do |f| %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email %>
  </div>

  <div class="field">
    <%= f.label :birthday %><br />
    <%= f.date_select :birthday %>
  </div>

  <div class="field">
    <%= f.label :sex %>
    <%= f.collection_select(:sex, Talent.sexes, :first, :first, { include_blank: true }, class:"form-control")  %>
  </div>

  <div class="field">
    <%= f.label :tel %><br />
    <%= f.number_field :tel %>
  </div>

  <div class="field">
    <%= f.label :edu_level %>
    <%= f.collection_select(:edu_level, Talent.edu_levels, :first, :first, { include_blank: true }, class:"form-control")  %>
  </div>

  <div class="field">
    <%= f.label :japanese_level %>
    <%= f.collection_select(:japanese_level, Talent.japanese_levels, :first, :first, { include_blank: true }, class:"form-control")  %>
  </div>

<!--  <div class="field">-->
    <%#= f.label :country %>
    <%#= f.collection_select(:country_id, Country.all, :id, :name, {prompt: ""},class:"form-control")  %>
<!--  </div>-->

<!--  <div class="field">-->
    <%#= f.label :visa %>
    <%#= f.collection_select(:visa_id, Visa.all, :id, :name, {prompt: ""},class:"form-control")  %>
<!--  </div>-->

<!--  <div class="field">-->
    <%#= f.label :prefecture %>
    <%#= f.collection_select(:prefecture_id, Prefecture.all, :id, :name, {prompt: ""},class:"form-control")  %>
<!--  </div>-->


  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </div>
  <div class="field">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation %>
  </div>

  <div class="form-group">
    <%= f.submit(class: "btn btn-primary") %>
  </div>

<% end %>